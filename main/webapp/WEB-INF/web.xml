<?xml version="1.0" encoding="UTF-8"?>
<web-app>
  
	<display-name>OnlineBanking</display-name>
	<welcome-file-list>
		
		<welcome-file>login.jsp</welcome-file>
		
	</welcome-file-list>

	<servlet>
		<servlet-name>serveletlogin</servlet-name>

		<servlet-class>serveletpack.ServeletLogin</servlet-class>
	</servlet>
	<servlet-mapping>

		<servlet-name>serveletlogin</servlet-name>
		<url-pattern> /ServeletLogin</url-pattern>

	</servlet-mapping>
	
	<servlet>
		<servlet-name>transferservlet</servlet-name>

		<servlet-class>accounttransfer.TransferServlet</servlet-class>
	</servlet>
	<servlet-mapping>

		<servlet-name>transferservlet</servlet-name>
		<url-pattern>/TransferServlet</url-pattern>

	</servlet-mapping>

	<servlet>
		<servlet-name>createaccountservelet</servlet-name>

		<servlet-class>newaccount.AccountServelet</servlet-class>
	</servlet>
	<servlet-mapping>

		<servlet-name>createaccountservelet</servlet-name>
<url-pattern>/AccountServelet</url-pattern>

	</servlet-mapping>
	
<servlet>
		<servlet-name>addcustomer</servlet-name>

		<servlet-class>customerservlet.AddingNewCustomer</servlet-class>
	</servlet>
	<servlet-mapping>

		<servlet-name>addcustomer</servlet-name>
<url-pattern>/AddingNewCustomer</url-pattern>

	</servlet-mapping>
	
	<servlet>
		<servlet-name>addaccount</servlet-name>

		<servlet-class>newaccount.AddingNewAccount</servlet-class>
	</servlet>
	<servlet-mapping>

		<servlet-name>addaccount</servlet-name>
<url-pattern>/AddingNewAccount</url-pattern>

	</servlet-mapping>
</web-app>
  
  
  
  
  
  
  
  
  
