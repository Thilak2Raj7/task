<?xml version="1.0" encoding="UTF-8"?>
<web-app>
  
	<display-name>OnlineBanking</display-name>
	<welcome-file-list>
		
		<welcome-file>login.jsp</welcome-file>
		
	</welcome-file-list>

	<servlet>
		<servlet-name>serveletlogin</servlet-name>

		<servlet-class>serveletpack.ServeletLogin</servlet-class>
	</servlet>
	<servlet-mapping>

		<servlet-name>serveletlogin</servlet-name>
		<url-pattern> /ServeletLogin</url-pattern>

	</servlet-mapping>
	
	<servlet>
		<servlet-name>transferservlet</servlet-name>

		<servlet-class>accounttransfer.TransferServlet</servlet-class>
	</servlet>
	<servlet-mapping>

		<servlet-name>transferservlet</servlet-name>
		<url-pattern>/TransferServlet</url-pattern>

	</servlet-mapping>

	<servlet>
		<servlet-name>createaccountservelet</servlet-name>

		<servlet-class>newaccount.AccountServelet</servlet-class>
	</servlet>
	<servlet-mapping>

		<servlet-name>createaccountservelet</servlet-name>
<url-pattern>/AccountServelet</url-pattern>

	</servlet-mapping>
	
<servlet>
		<servlet-name>addcustomer</servlet-name>

		<servlet-class>customerservlet.AddingNewCustomer</servlet-class>
	</servlet>
	<servlet-mapping>

		<servlet-name>addcustomer</servlet-name>
<url-pattern>/AddingNewCustomer</url-pattern>

	</servlet-mapping>
	
	<servlet>
		<servlet-name>addaccount</servlet-name>

		<servlet-class>newaccount.AddingNewAccount</servlet-class>
	</servlet>
	<servlet-mapping>

		<servlet-name>addaccount</servlet-name>
<url-pattern>/AddingNewAccount</url-pattern>

	</servlet-mapping>
	
	<servlet>
		<servlet-name>cashdeposit</servlet-name>

		<servlet-class>accounttransfer.CashDeposit</servlet-class>
	</servlet>
	<servlet-mapping>

		<servlet-name>cashdeposit</servlet-name>
<url-pattern>/CashDeposit</url-pattern>

	</servlet-mapping>
	
	<servlet>
		<servlet-name>cashwithdraw</servlet-name>

		<servlet-class>accounttransfer.CashWithdrawal</servlet-class>
	</servlet>
	<servlet-mapping>

		<servlet-name>cashwithdraw</servlet-name>
<url-pattern>/CashWithdrawal</url-pattern>

	</servlet-mapping>
	
	
	
	<servlet>
		<servlet-name>update</servlet-name>

		<servlet-class>newAccount.Update</servlet-class>
	</servlet>
	<servlet-mapping>

		<servlet-name>update</servlet-name>
<url-pattern>/Update</url-pattern>

	</servlet-mapping>
	
	<servlet>
		<servlet-name>updateaccount</servlet-name>

		<servlet-class>newAccount.UpdateAccount</servlet-class>
	</servlet>
	<servlet-mapping>

		<servlet-name>updateaccount</servlet-name>
<url-pattern>/UpdateAccount</url-pattern>

	</servlet-mapping>
	
	<servlet>
		<servlet-name>deactivate</servlet-name>

		<servlet-class>newAccount.Deactivate</servlet-class>
	</servlet>
	<servlet-mapping>

		<servlet-name>udeactivate</servlet-name>
<url-pattern>/Deactivate</url-pattern>

	</servlet-mapping>
	
</web-app>

  
  
  
  
  
  
  
  
  
